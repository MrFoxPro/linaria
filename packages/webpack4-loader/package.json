  },
  "devDependencies": {
    "@types/enhanced-resolve": "^3.0.6",
    "@types/loader-utils": "^1.1.3",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^17.0.39",
    "@types/webpack": "^4.41.33",
    "linaria-scripts": "workspace:^1.0.0",
    "source-map": "^0.7.3"
  },
  "peerDependencies": {
    "webpack": ">=4.0.0 <5.0.0"
